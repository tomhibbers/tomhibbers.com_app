{
  "cli": {
    "version": ">= 0.54.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "dev": {
      "distribution": "internal",
      "android": {
        "autoIncrement": "version"
      }
    },
    "store": {
      "android": {
        "autoIncrement": "versionCode"
      }
    },
    "production": {
      "android": {
        "autoIncrement": "versionCode"
      }
    }
  },
  "submit": {
    "uat": {
      "android": {
        "serviceAccountKeyPath": ".eas-publish/android.json",
        "track": "internal",
        "releaseStatus": "completed"
      }
    }
  }
}
