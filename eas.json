{
  "cli": {
    "version": ">= 0.54.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "aap": {
      "android": {
        "autoIncrement": "versionCode"
      }
    },
    "apk": {
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "android": {
        "autoIncrement": "versionCode"
      }
    }
  },
  "submit": {
    "aap": {
      "android": {
        "serviceAccountKeyPath": ".eas-publish/android.json",
        "track": "alpha",
        "releaseStatus": "draft"
      }
    }
  }
}
